name: 'Freemarker'
scopeName: 'Freemarker'
type: 'tree-sitter'
parser: 'tree-sitter-freemarker'

fileTypes: [
  'ftl'
]

injectionRegExp: '(FTL|ftl|Ftl)$',

folds: [
  {
    type: ['start_tag', 'raw_start_tag', 'self_closing_tag'],
    start: {index: 1},
    end: {index: -1}
  }
  {
    type: ['element', 'raw_element'],
    start: {index: 1},
    end: {index: -1}
  }
]

comments:
  start: '<#--'
  end: '-->'
